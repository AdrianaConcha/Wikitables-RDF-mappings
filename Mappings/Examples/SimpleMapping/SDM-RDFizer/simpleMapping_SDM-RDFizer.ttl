<SimpleMapping>
  a rr:TriplesMap;
    
  rml:logicalSource [
    rml:source "../Input/simpleMapping.csv";
    rml:referenceFormulation ql:CSV
  ];
  rr:subjectMap [
    rr:template "http://www.wikidata.org/entity/{Candidate}";
    
  ]; 
  rr:predicateObjectMap [ 
    rr:predicate wdt:P102; 
    rr:objectMap [
	rr:template "http://www.wikidata.org/entity/{2_Party}";
    ];
  ].

