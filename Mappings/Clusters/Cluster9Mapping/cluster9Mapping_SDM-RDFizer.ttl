<#Cluster9Mapping> a rr:TriplesMap;
	rml:logicalSource [
		rml:source "../Input/cluster9.csv";
    		rml:referenceFormulation ql:CSV
  	];
  	rr:subjectMap [
    		rr:template "http://www.wikidata.org/entity/{Title_3}";
   	];

  	rr:predicateObjectMap [
    		rr:predicate wdt:P676;
    		rr:objectMap [
      			rr:template "http://www.wikidata.org/entity/{Writer_3}"
    		]
    	];
  	rr:predicateObjectMap [
    		rr:predicate wdt:P31;
    		rr:objectMap [
      			rr:constant wd:Q105543609
    		]
    	];
  	rr:predicateObjectMap [
    		rr:predicate wdt:P361;
    		rr:objectMap [
      			rr:template "http://www.wikidata.org/entity/{ArticleEntity}"
    		]
    	];
  	rr:predicateObjectMap [
    		rr:predicate wdt:P7937;
    		rr:objectMap [
      			rr:constant wd:Q7366
    		]
    	]. 

<#Writer> a rr:TriplesMap;
