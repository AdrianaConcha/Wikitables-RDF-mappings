
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wds: <http://www.wikidata.org/entity/statement/>
PREFIX wdv: <http://www.wikidata.org/value/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX  xyz:  <http://sparql.xyz/facade-x/data/>
PREFIX  fx:   <http://sparql.xyz/facade-x/ns/>

CONSTRUCT
  {
    ?games wdt:P156 ?games_next. 
  }
WHERE
  { SERVICE <x-sparql-anything:location=../Input/cluster90preprocessed.csv,csv.headers=true> {
fx:properties  fx:csv.null-string  "" .
        ?c        xyz:gameID1 ?gameID1;
		  xyz:gameID2 ?gameID2.
}
BIND(REPLACE(?gameID1, " ", "") as ?gameID1trim)
BIND(REPLACE(?gameID2, " ", "") as ?gameID2trim)
 BIND(REPLACE(?gameID1trim,",Q[0-9]*","") AS ?firstGameID1)
 BIND(REPLACE(?gameID2trim,",Q[0-9]*","") AS ?firstGameID2)
BIND (URI(CONCAT('http://www.wikidata.org/entity/', ?firstGameID1)) AS ?games)
BIND (URI(CONCAT('http://www.wikidata.org/entity/', ?firstGameID2)) AS ?games_next)
 
  }

