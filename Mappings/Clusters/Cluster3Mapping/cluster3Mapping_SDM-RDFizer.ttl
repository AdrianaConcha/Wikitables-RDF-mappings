@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix transit: <http://vocab.org/transit/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix wd: <http://www.wikidata.org/entity/>.
@prefix wds: <http://www.wikidata.org/entity/statement/>.
@prefix wdv: <http://www.wikidata.org/value/>.
@prefix wdt: <http://www.wikidata.org/prop/direct/>.
@prefix p: <http://www.wikidata.org/prop/>.
@prefix ps: <http://www.wikidata.org/prop/statement/>.
@prefix pq: <http://www.wikidata.org/prop/qualifier/>.


<#CLuster3Mapping> a rr:TriplesMap;
	rml:logicalSource [
		rml:source "../Input/cluster3.csv";
		rml:referenceFormulation ql:CSV
  	];
  	rr:subjectMap [
    		rr:template "http://www.wikidata.org/entity/{ArticleEntity}";
   
 	 ];

  	rr:predicateObjectMap [
    		rr:predicate p:P444;
    		rr:objectMap [
       			rr:termType rr:BlankNode ;
       			rr:template "source{ArticleEntity}{ArticleTitle}{Review_score__rate_3}{Review_score__Source_3}"
    		]
    	].

<#Review> a rr:TriplesMap;
 	rml:logicalSource [
    		rml:source "../Input/cluster3.csv";
    		rml:referenceFormulation ql:CSV
  	];
  	rr:subjectMap [
    		rr:termType rr:BlankNode;
    		rr:template "source{ArticleEntity}{ArticleTitle}{Review_score__rate_3}{Review_score__Source_3}"
	];

  	rr:predicateObjectMap [
		rr:predicate ps:P444;
    		rr:objectMap [
      			rml:reference "Review_score__rate_3"
		]
    	];
	rr:predicateObjectMap [
    		rr:predicate pq:P447;
    		rr:objectMap [
       			rr:template "http://www.wikidata.org/entity/{Review_score__Source_3}"
		]
    	].


