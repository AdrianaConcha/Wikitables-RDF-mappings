<#CLuster3Mapping> a rr:TriplesMap;
	rml:logicalSource [
		rml:source "../Input/cluster3.csv";
		rml:referenceFormulation ql:CSV
  	];
  	rr:subjectMap [
    		rr:template "http://www.wikidata.org/entity/{ArticleEntity}";
   
 	 ];

  	rr:predicateObjectMap [
    		rr:predicate p:P444;
    		rr:objectMap [
       			rr:termType rr:BlankNode ;
       			rr:template "_:source{ArticleEntity}{ArticleTitle}{Review_score__rate_3}{Review_score__Source_3}"
    		]
    	].

<#Review> a rr:TriplesMap;
 	rml:logicalSource [
    		rml:source "../Input/cluster3.csv";
    		rml:referenceFormulation ql:CSV
  	];
  	rr:subjectMap [
    		rr:termType rr:BlankNode;
    		rr:template "_:source{ArticleEntity}{ArticleTitle}{Review_score__rate_3}{Review_score__Source_3}"
	];

  	rr:predicateObjectMap [
		rr:predicate ps:P444;
    		rr:objectMap [
      			 rml:functionExecution <#Execution>
		]
    	];
	rr:predicateObjectMap [
    		rr:predicate pq:P447;
    		rr:objectMap [
       			rr:template "http://www.wikidata.org/entity/{Review_score__Source_3}"
		]
    	].




<#Execution> a rml:Execution ;
    rml:function grel:string_trim ;
    rml:input
        [
            rml:parameter grel:valueParam ;
            rml:inputValueMap [
                rml:reference "Review_score__rate_3"
		
            ]
        ] .







